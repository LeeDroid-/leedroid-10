    <module name="MessageConfig">
      <function name="Message_QuickText">
        <set name="plenty">
          <item name="Locale">en_US</item>
          <item name="Quick_Text1">Can't talk right now. Send me a message.</item>
          <item name="Quick_Text2">Call me</item>
          <item name="Quick_Text3">Where are you?</item>
          <item name="Quick_Text4">Can you talk?</item>
          <item name="Quick_Text5">Don't forget to</item>
          <item name="Quick_Text6">Can you pick up</item>
          <item name="Quick_Text7">Thanks!</item>
          <item name="Quick_Text8">Meet me at</item>
          <item name="Quick_Text9">Let's get lunch.</item>
          <item name="Quick_Text10">I sent this from my PCS phone.</item>
          <item name="Quick_Text11">The meeting has been cancelled.</item>
          <item name="Quick_Text12">I have to work late.</item>
          <item name="Quick_Text13">I'm on a conference call.</item>
          <item name="Quick_Text14">I'll be there at</item>
          <item name="Quick_Text15">What time does it start?</item>
          <item name="Quick_Text16">Tag, you're it!</item>
          <item name="Quick_Text17">I love you!</item>
          <item name="Quick_Text18">Hugs and kisses - OXOXO</item>
          <item name="Quick_Text19">Crazy 4 you!</item>
          <item name="Quick_Text20">Too funny!</item>
        </set>
        <set name="plenty">
          <item name="Locale">es_US</item>
          <item name="Quick_Text1">No puedo hablar ahora. Envíame un mensaje.</item>
          <item name="Quick_Text2">Llámame.</item>
          <item name="Quick_Text3">¿Dónde estás?</item>
          <item name="Quick_Text4">¿Puedes hablar?</item>
          <item name="Quick_Text5">No te olvides de</item>
          <item name="Quick_Text6">¿Podemos vernos hoy?</item>
          <item name="Quick_Text7">¡Gracias!</item>
          <item name="Quick_Text8">Nos vemos en</item>
          <item name="Quick_Text9">¡Vamos a comer!</item>
          <item name="Quick_Text10">Envié esto desde mi teléfono PCS.</item>
          <item name="Quick_Text11">La reunión ha sido cancelada.</item>
          <item name="Quick_Text12">Tengo que trabajar hasta tarde.</item>
          <item name="Quick_Text13">Estoy en una llamada de conferencia.</item>
          <item name="Quick_Text14">Estaré ahí a las</item>
          <item name="Quick_Text15">¿A qué hora empieza?</item>
          <item name="Quick_Text16">¡Qué mala onda!</item>
          <item name="Quick_Text17">¡Te quiero!</item>
          <item name="Quick_Text18">Muchos besos,</item>
          <item name="Quick_Text19">¡Te extraño!</item>
          <item name="Quick_Text20">¡Me muero de la risa!</item>
        </set>
      </function>
